{
  "name": "tumi",
  "license": "MIT",
  "private": true,
  "version": "4.0.0",
  "workspaces": [
    "events",
    "server",
    "experiments",
    "shared/*"
  ],
  "scripts": {
    "events": "yarn workspace @tumi/events dev",
    "server": "yarn workspace @tumi/server dev",
    "experiments": "yarn workspace @tumi/experiments dev",
    "start": "concurrently --kill-others-on-fail \"yarn server\"  \"yarn events\""
  },
  "packageManager": "yarn@3.1.1",
  "dependencies": {
    "concurrently": "^7.0.0"
  },
  "resolutions": {
    "@graphql-codegen/typescript-apollo-angular@3.3.3": "patch:@graphql-codegen/typescript-apollo-angular@npm:3.3.3#.yarn/patches/@graphql-codegen-typescript-apollo-angular-npm-3.3.3-d3ae1f4c6e"
  }
}
