{
  "name": "@tumi/party-animals",
  "private": true,
  "description": "Remix app for party animals registration and management",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn prisma generate && yarn build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"yarn dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.0",
    "@prisma/client": "^3.13.0",
    "@remix-run/node": "^1.4.1",
    "@remix-run/react": "^1.4.1",
    "@remix-run/serve": "^1.4.1",
    "@tailwindcss/forms": "^0.5.0",
    "cross-env": "^7.0.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "remix-auth": "^3.2.1",
    "remix-auth-auth0": "1.3.6",
    "stripe": "^8.219.0",
    "v8n": "^1.4.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.2",
    "autoprefixer": "^10.4.5",
    "concurrently": "^7.1.0",
    "postcss": "^8.4.12",
    "prisma": "^3.13.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  }
}
